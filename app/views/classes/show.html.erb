<div class="page-header">
  <h3>Class #<%= @class.id %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>Course</dt>
      <dd><%= @class.course_id %></dd>

      <dt>Professor</dt>
      <dd><%= @class.professor_id %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/classes" class="btn btn-primary">
        Back
      </a>
      <a href="/classes/<%= @class.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_class/<%= @class.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
